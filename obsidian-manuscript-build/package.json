{
	"name": "obsidian-md-manuscript",
	"version": "1.0.0",
	"description": "Build academic manuscripts to PDF/DOCX using Pandoc from within Obsidian",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"pandoc",
		"markdown",
		"manuscript",
		"pdf",
		"docx",
		"academic",
		"writing"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^18.19.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.19.0",
		"obsidian": "latest",
		"tslib": "^2.6.0",
		"typescript": "^5.3.0"
	}
}
